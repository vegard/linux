config MODULES
  bool "Enable loadable module support"
  modules

config CRYPTO_ALGAPI2
  tristate

config CRYPTO_HASH2
  tristate
  select CRYPTO_ALGAPI2

config CRYPTO_MANAGER2
  tristate
  default m
  select CRYPTO_HASH2

